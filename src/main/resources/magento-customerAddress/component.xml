<component name="magento-customerAddress" type="synapse/template">
    <subComponents>
        <component name="createCustomerAddress">
            <file>createCustomerAddress.xml</file>
            <description>Create a new address for a customer.</description>
        </component>
        <component name="getCustomerAddressInfo">
            <file>getCustomerAddressInfo.xml</file>
            <description>Retrieve the specified customer address.</description>
        </component>
        <component name="updateCustomerAddress">
            <file>updateCustomerAddress.xml</file>
            <description>Update the customer address.</description>
        </component>
        <component name="deleteCustomerAddress">
            <file>deleteCustomerAddress.xml</file>
            <description>Delete the customer address.</description>
        </component>
    </subComponents>
</component>